    "from gtts import gTTS\n",
    "import os\n",
    "file = open(\"hack_trial.txt\",\"r\").read().replace(\"\\n\",\" \")\n",
    "language = 'en'\n",
    "speech = gTTS(text = str(file), lang = language, slow = False)\n",
    "speech.save(\"trial.mp3\")\n",
    "os.system(\"start trial.mp3\")"
   ]
